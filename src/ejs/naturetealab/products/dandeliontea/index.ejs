                  <%- imgTag("products/dandelion/dandelion-02-main.jpg", "yo", false,path)%>
                </div>
                <div class="swiper-slide p-detail__slide">
                  <%- imgTag("products/dandelion/dandelion-03-main.jpg", "bi", false,path)%>
                </div>
                <div class="swiper-slide p-detail__slide">
                  <%- imgTag("products/dandelion/dandelion-04-main.jpg", "dsc", false,path)%>
                </div>
              </div>
            </div>

            <!-- thumbnail -->
            <div class="swiper-container p-detail-thumbnail">
              <div class="swiper-wrapper">
                <div class="swiper-slide p-detail__slide">
                  <%- imgTag("products/products-03.jpg", "DANDELION", false,path)%>
